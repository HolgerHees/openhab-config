Bridge fineoffsetweatherstation:gateway:default "Wetterstation Bridge" [
     ip="{{custom_weatherstation_ip}}",
     port="45000",
     discoverInterval="900",
     pollingInterval="16",
     protocol="DEFAULT"
] {
 Thing sensor WH90 "Wetterstation Haupt Sensor" [sensor="WH90"]
 Thing sensor WH26 "Wetterstation Extern Sensor" [sensor="WH26"]
}
